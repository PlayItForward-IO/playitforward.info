      }
    },
    "agx-store": {
      "root": "libs/agx-store",
      "sourceRoot": "libs/agx-store/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/agx-store",
            "main": "libs/agx-store/src/index.ts",
            "tsConfig": "libs/agx-store/tsconfig.lib.json",
