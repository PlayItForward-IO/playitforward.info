{
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "statusBar.background": "#b85833",
    "statusBarItem.hoverBackground": "#ce7350",
    "statusBar.foreground": "#e7e7e7",
    "statusBar.border": "#b85833"
  },
  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/graph": true
  },
  "cSpell.words": [
    "Acerca",
    "Autosize",
    "Bienvenido",
    "Bienvenue",
    "Buffay",
    "Buildable",
    "Datepicker",
    "Deutsch",
    "ETFS",
    "FULLSCREEN",
    "Forex",
    "Fran",
    "Français",
    "Gmail",
    "Neekman",
    "Neekware",
    "PFWD",
    "Podcasters",
    "Postgres",
    "SEEKRET",
    "Serializers",
    "Sidenav",
    "Singup",
    "Subify",
    "Tribbiani",
    "Unprocessable",
    "ais",
    "asynciterable",
    "biesbjerg",
    "bluegrey",
    "cachify",
    "createdb",
    "dbml",
    "deeppurple",
    "devkit",
    "eventview",
    "fontawesome",
    "fullspan",
    "gtag",
    "herzlich",
    "hotspot",
    "maxlength",
    "minlength",
    "nestjs",
    "nrwl",
    "packagr",
    "pageview",
    "pinst",
    "playitforward",
    "rehydrated",
    "screenfull",
    "sidenav",
    "signup",
    "submenu",
    "tailwindcss",
    "tryget",
    "typegraphql",
    "upsert",
    "willkommen",
